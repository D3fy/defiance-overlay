# conf.d file for bolo2meta

USER="bolo"
GROUP="bolo"

# The endpoint is the bolo subscriber endpoint
# Where bolo2meta connectes to and looks for events
ENDPOINT="tcp://127.0.0.1:2999"

# bolo listener endpoint for perf metrics
SUBMIT="tcp://127.0.0.1:2998"

# prefix for submitting perf metrics back
PREFIX="meta"
